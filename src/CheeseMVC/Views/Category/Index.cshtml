@model List<CheeseMVC.Models.CheeseCategory>

<h2>Index</h2>

@{ 
<ul>
    <p><a asp-controller="Category" asp-action="Add">Add Category</a></p>
    @foreach (var category in Model)
    {
        <li>
            <a asp-controller="List" asp-action="Category">@category.Name</a>
        </li>

    }
</ul>
    ViewData["Title"] = "Index";
}


